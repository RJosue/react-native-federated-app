{
  "name": "host",
  "version": "0.0.1",
  "private": true,
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "android": "react-native run-android --no-packager",
    "ios": "react-native run-ios --no-packager",
    "start:app": "react-native webpack-start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "install:pods": "cd ios && pod install",
    "install:pods:update": "cd ios && pod install --repo-update && cd ..",
    "build:ios": "react-native webpack-bundle --dev false --platform ios --entry-file ./index.js --bundle-output ./ios/main.jsbundle"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/pro-duotone-svg-icons": "^6.3.0",
    "@fortawesome/pro-light-svg-icons": "^6.3.0",
    "@fortawesome/pro-regular-svg-icons": "^6.3.0",
    "@fortawesome/pro-solid-svg-icons": "^6.3.0",
    "@fortawesome/pro-thin-svg-icons": "^6.3.0",
    "@fortawesome/react-native-fontawesome": "^0.3.0",
    "@fortawesome/sharp-regular-svg-icons": "^6.3.0",
    "@fortawesome/sharp-solid-svg-icons": "^6.3.0",
    "@nassa/video-call": "^0.3.0-103626",
    "@react-native-community/cli": "^10.1.3",
    "@react-native-community/cli-types": "^10.0.0",
    "@react-native-firebase/app": "^16.5.1",
    "@react-native-firebase/messaging": "^16.5.1",
    "@react-native-masked-view/masked-view": "^0.2.7",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.4",
    "graphql": "^16.6.0",
    "react": "18.1.0",
    "react-native": "0.70.3",
    "react-native-device-info": "^10.3.0",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-reanimated": "^2.14.4",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-svg": "^13.8.0",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@callstack/repack": "^3.1.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.4",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
